{
  "name": "abacaphiliac/doctrine-orm-diagnostics-module",
  "description": "Integration of doctrine/doctrine-orm-module, zendframework/zenddiagnostics, and (optionally) doctrine/migrations.",
  "license": "MIT",
  "authors": [
    {
      "name": "Timothy Younger",
      "email": "abacaphiliac@gmail.com"
    }
  ],
  "keywords": [
    "doctrine",
    "orm",
    "module",
    "zf2",
    "zendframework",
    "diagnostics"
  ],
  "require": {
    "php": ">=5.5",
    "doctrine/dbal": "^2.5.4",
    "doctrine/doctrine-orm-module": "^0.10.0",
    "symfony/console": "^3.0",
    "zendframework/zenddiagnostics": "^1.0",
    "zendframework/zftool": "dev-master",
    "zendframework/zend-console": "^2.0",
    "zendframework/zend-modulemanager": "^2.0",
    "doctrine/doctrine-module": "^1.0"
  },
  "require-dev": {
    "doctrine/migrations": "dev-master",
    "jakub-onderka/php-parallel-lint": "^0.9",
    "mikey179/vfsStream": "^1.6",
    "phing/phing": "^2.9",
    "phpunit/phpunit": "^5.4|^4.8",
    "squizlabs/php_codesniffer": "^2.2",
    "zendframework/zendframework": "^2.0",
    "zendframework/zend-test": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Abacaphiliac\\DoctrineORMDiagnosticsModule\\": "src/DoctrineORMDiagnosticsModule"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Abacaphiliac\\DoctrineORMDiagnosticsModuleTest\\": "tests/DoctrineORMDiagnosticsModuleTest",
      "DoctrineORMModuleTest\\": "vendor/doctrine/doctrine-orm-module/tests/DoctrineORMModuleTest"
    }
  }
}
